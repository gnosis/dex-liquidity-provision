{
  "name": "liquidity-provision-scripts",
  "version": "0.1.0",
  "description": "",
  "devDependencies": {
    "@gnosis.pm/mock-contract": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-no-only-tests": "^2.4.0",
    "eslint-plugin-react": "^7.19.0",
    "eth-lightwallet": "^4.0.0",
    "ethereumjs-abi": "^0.6.5",
